#include<stdio.h>                                // ROLL NUMBER : 19A81A05N8 //
void selectionsort(int a[],int i,int n);
int main()
{
    int a[100],i,n;
    printf("Enter n value");
    scanf("%d",&n);
    
    for(i=1;i<=n;i++)
    {                               
        printf("Enter %d element ",i);
        scanf("%d",&a[i]);
    }
    
    selectionsort(a,1,n);
    
    printf("After sorting\n");
    for(i=1;i<=n;i++)
    {
        printf("%d  ",a[i]);
    }
    return 0;
}

void selectionsort(int a[],int i,int n)
{
    int j,t;
    for(i=1;i<=n;i++)
    {
        for(j=i+1;j<=n;j++)
        {
        
          if(a[i]>a[j])
           {
              t=a[i];
              a[i]=a[j];
              a[j]=t;
           }
        }
    }
}
